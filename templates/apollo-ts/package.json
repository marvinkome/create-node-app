{
    "name": "create-node-app",
    "version": "1.0.0",
    "main": "index.ts",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start": "node -r module-alias/register -r dotenv/config  ./dist/index.js",
        "dev": "nodemon --exec ts-node -r dotenv/config -r tsconfig-paths/register index.ts"
    },
    "dependencies": {
        "apollo-server-express": "^2.6.1",
        "bcrypt": "^5.0.0",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-jwt": "^5.3.1",
        "graphql": "^14.3.1",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.0",
        "mongoose": "^5.5.13",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.0",
        "@types/express-jwt": "^0.0.42",
        "@types/graphql": "^14.2.0",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/mongoose": "^5.5.5",
        "@types/passport": "^1.0.0",
        "@types/passport-local": "^1.0.33",
        "tsconfig-paths": "^3.8.0",
        "typescript": "^3.5.1"
    },
    "_moduleAliases": {
        "@gql": "./dist/src/gql",
        "@libs": "./dist/src/libs",
        "@models": "./dist/src/models",
        "@routes": "./dist/src/routes"
    }
}
